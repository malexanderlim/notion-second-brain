{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "api/main.py",
      "use": "@vercel/python"
    }
  ],
  "functions": {
    "api/main.py": {
      "excludeFiles": "**/__pycache__/**,**/*.pyc,.git/**,frontend/**,tests/**,venv/**,notion_second_brain/**,scripts/**,output/**,docs/**,.cursor/**,tasks/**,design/**,*.md,*.sh,*.py,*.log,*.m4a,index.faiss,index_mapping.json,metadata_cache.json,schema.json,last_entry_update_timestamp.txt,.gitignore,.DS_Store"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/main.py"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 